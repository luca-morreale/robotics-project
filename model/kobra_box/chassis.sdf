<?xml version="1.0" ?>
<sdf version="1.6">

<model name="kobra_chassis">
    <link name="chassis">
        <!-- Offset the base by slightly more then half the height of the wheel -->
        <pose>0 0 0.15 0 0 0</pose>
	<inertial>
        <mass>9.5</mass>
	<!-- matrix of inertias of a solid cuboid with mass m, width w, 
	     height h, depth d is:
		|m(h^2+d^2)/12		0		0    |
		|	0	m(w^2+d^2)/12		0    |
		|	0		0	m(w^2+h^2)/12| 
	-->
        <inertia>
          <ixx>0.21179</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.3</iyy>
          <iyz>0.0</iyz>
          <izz>0.478978</izz>
        </inertia>
      </inertial>
        <collision name="collision">
            <geometry>
            <box>
                <!-- Dimensions provided by Nuzoo Robotics -->
                <size>0.6 0.495 0.15</size>
            </box>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <box>
                <!-- Dimensions provided by Nuzoo Robotics -->
                <size>0.6 0.495 0.15</size>
            </box>
            </geometry>
        </visual>
    </link>
    
    <link name="right_front_wheel">
        <!-- x y z roll pitch yaw 
        roll 90Â°=1.5707 rad      
        -->
        <pose>0.09 -0.2825 0.0725 1.5707 0 0</pose>
	<inertial>
        <mass>1</mass>
	<!-- matrix of inertias of a solid cylinider with mass m,
	     radius r, height h is:
		|m(3*r^2+h^2)/12	0	   0    |
		|	0	m(3*r^2+h^2)/12	   0    |
		|	0		0	m*r^2/2 | 
	-->
        <inertia>
          <ixx>0.00148</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.00148</iyy>
          <iyz>0.0</iyz>
          <izz>0.002628</izz>
        </inertia>
      </inertial>
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="left_front_wheel">
        <pose>0.09 0.2825 0.0725 1.5707 0 0</pose>
	<inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.00148</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.00148</iyy>
          <iyz>0.0</iyz>
          <izz>0.002628</izz>
        </inertia>
      </inertial>
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="right_rear_wheel">
        <pose>-0.09 -0.2825 0.0725 1.5707 0 0</pose>
	<inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.00148</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.00148</iyy>
          <iyz>0.0</iyz>
          <izz>0.002628</izz>
        </inertia>
      </inertial>
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="left_rear_wheel">
        <pose>-0.09 0.2825 0.0725 1.5707 0 0</pose>
	<inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.00148</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.00148</iyy>
          <iyz>0.0</iyz>
          <izz>0.002628</izz>
        </inertia>
      </inertial>
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <!--                    Joints                       -->
    
    <joint type="revolute" name="right_front_wheel_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>right_front_wheel</child>
        <parent>chassis</parent>
        <axis>
            <xyz>0 0 1</xyz>
        </axis>
    </joint>
    <joint type="revolute" name="right_rear_wheel_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>right_rear_wheel</child>
        <parent>chassis</parent>
        <axis>
            <xyz>0 0 1</xyz>
        </axis>
    </joint>
    <joint type="revolute" name="left_front_wheel_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>left_front_wheel</child>
        <parent>chassis</parent>
        <axis>
            <xyz>0 0 1</xyz>
        </axis>
    </joint>
    <joint type="revolute" name="left_rear_wheel_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>left_rear_wheel</child>
        <parent>chassis</parent>
        <axis>
            <xyz>0 0 1</xyz>
        </axis>
    </joint>
    

    
</model>
</sdf>

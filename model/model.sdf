<?xml version="1.0" ?>
<sdf version="1.6">

<model name="Kobra">
    <link name="chassis">
        <!-- Offset the base by slightly more then half the height of the wheel -->
        <pose>0 0 0.15 0 0 0</pose>
        <inertial> 
            <mass>9.5</mass> 
            <!-- matrix of inertias of a solid cuboid with mass m, width w,  
                height h, depth d is: 
                | m(h^2+d^2)/12      0            0        | 
                |  0            m(w^2+d^2)/12     0        | 
                |  0                  0       m(w^2+h^2)/12|  
            --> 
            <inertia> 
                <ixx>0.21179</ixx> 
                <ixy>0.0</ixy> 
                <ixz>0.0</ixz> 
                <iyy>0.3</iyy> 
                <iyz>0.0</iyz> 
                <izz>0.478978</izz> 
            </inertia> 
        </inertial> 
        <collision name="collision">
            <geometry>
            <box>
                <!-- Dimensions provided by Nuzoo Robotics -->
                <size>0.6 0.495 0.15</size>
            </box>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <box>
                <!-- Dimensions provided by Nuzoo Robotics -->
                <size>0.6 0.495 0.15</size>
            </box>
            </geometry>
        </visual>
    </link>
    
    <link name="vertical_arm">
        <!-- Offset the arm by half the height of the wheel +
        + the height of the chassis -->
        <pose>0.1625 0 0.675 0 0 0</pose>
        <inertial> 
            <mass>0.5</mass> 
            <!-- matrix of inertias of a solid cylinider with mass m, 
                radius r, height h is: 
                |m(3*r^2+h^2)/12      0               0     | 
                |  0              m(3*r^2+h^2)/12     0     | 
                |  0                 0              m*r^2/2 |  
            --> 
            <inertia> 
            <ixx>0.033828</ixx> 
            <ixy>0.0</ixy> 
            <ixz>0.0</ixz> 
            <iyy>0.033828</iyy> 
            <iyz>0.0</iyz> 
            <izz>0.000156</izz> 
            </inertia> 
        </inertial> 
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>.025</radius>
                <length>.9</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>.025</radius>
                <length>.9</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="arm_extension">
        <pose>0.1875 0 1.0975 0 -1.5707 0</pose>
        <inertial> 
            <mass>.1</mass> 
            <!-- matrix of inertias of a solid cylinider with mass m, 
                radius r, height h is: 
                | m(3*r^2+h^2)/12  0                0    | 
                |  0           m(3*r^2+h^2)/12      0    | 
                |  0            0                m*r^2/2 |  
            --> 
            <inertia> 
            <ixx>0.0083125</ixx> 
            <ixy>0.0</ixy> 
            <ixz>0.0</ixz> 
            <iyy>0.0083125</iyy> 
            <iyz>0.0</iyz> 
            <izz>0.078125</izz> 
            </inertia> 
        </inertial> 
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>.01</radius>
                <length>.03</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>.01</radius>
                <length>.03</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="camera_junction_sphere">
        <pose>0.211 0 1.0975 0 0 0</pose>
        <inertial> 
            <mass>.1</mass> 
            <!-- matrix of inertias of a solid sphere with mass m and radius r: 
                |(2*m*r^2)/5         0            0    | 
                |  0             (2*m*r^2)/5      0    | 
                |  0                  0     (2*m*r^2)/5|  
            --> 
            <inertia> 
                <ixx>0.00000625</ixx> 
                <ixy>0</ixy> 
                <ixz>0.0</ixz> 
                <iyy>0.00000625</iyy> 
                <iyz>0.0</iyz> 
                <izz>0.00000625</izz> 
            </inertia> 
        </inertial> 
        <collision name="collision">
            <geometry>
            <sphere>
                <radius>.0125</radius>
            </sphere>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <sphere>
                <radius>.0125</radius>
            </sphere>
            </geometry>
        </visual>
    </link>
    
    <link name="camera_support">
        <pose>0.222 0 1.0915 0 2.0943 0</pose>
        <inertial> 
            <mass>.1</mass> 
            <inertia> 
                <ixx>0.0083125</ixx> 
                <ixy>0.0</ixy> 
                <ixz>0.0</ixz> 
                <iyy>0.0083125</iyy> 
                <iyz>0.0</iyz> 
                <izz>0.078125</izz> 
            </inertia> 
        </inertial> 
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>.01</radius>
                <length>.01</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>.01</radius>
                <length>.01</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="right_front_wheel">
        <!-- x y z roll pitch yaw 
        roll 90Â°=1.5707 rad      
        -->
        <pose>0.09 -0.2825 0.0725 1.5707 0 0</pose>
        <inertial> 
            <mass>1</mass> 
            <!-- matrix of inertias of a solid cylinider with mass m, 
                radius r, height h is: 
                |m(3*r^2+h^2)/12  0                  0    | 
                |  0             m(3*r^2+h^2)/12     0    | 
                |  0                0             m*r^2/2 |  
            --> 
            <inertia> 
                <ixx>0.00148</ixx> 
                <ixy>0.0</ixy> 
                <ixz>0.0</ixz> 
                <iyy>0.00148</iyy> 
                <iyz>0.0</iyz> 
                <izz>0.002628</izz> 
            </inertia> 
        </inertial> 
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="left_front_wheel">
        <pose>0.09 0.2825 0.0725 1.5707 0 0</pose>
        <inertial> 
            <mass>1</mass> 
            <inertia> 
                <ixx>0.00148</ixx> 
                <ixy>0.0</ixy> 
                <ixz>0.0</ixz> 
                <iyy>0.00148</iyy> 
                <iyz>0.0</iyz> 
                <izz>0.002628</izz> 
            </inertia> 
        </inertial> 
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="right_rear_wheel">
        <pose>-0.09 -0.2825 0.0725 1.5707 0 0</pose>
        <inertial> 
            <mass>1</mass> 
            <inertia> 
                <ixx>0.00148</ixx> 
                <ixy>0.0</ixy> 
                <ixz>0.0</ixz> 
                <iyy>0.00148</iyy> 
                <iyz>0.0</iyz> 
                <izz>0.002628</izz> 
            </inertia> 
        </inertial> 
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <link name="left_rear_wheel">
        <pose>-0.09 0.2825 0.0725 1.5707 0 0</pose>
        <inertial> 
            <mass>1</mass> 
            <inertia> 
                <ixx>0.00148</ixx> 
                <ixy>0.0</ixy> 
                <ixz>0.0</ixz> 
                <iyy>0.00148</iyy> 
                <iyz>0.0</iyz> 
                <izz>0.002628</izz> 
            </inertia> 
        </inertial>
        <collision name="collision">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
            <cylinder>
                <radius>0.0725</radius>
                <length>0.045</length>
            </cylinder>
            </geometry>
        </visual>
    </link>
    
    <!--                    Joints                       -->
    
    <joint type="revolute" name="right_front_wheel_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>right_front_wheel</child>
        <parent>chassis</parent>
        <axis>
            <xyz>0 0 1</xyz>
        </axis>
    </joint>
    <joint type="revolute" name="right_rear_wheel_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>right_rear_wheel</child>
        <parent>chassis</parent>
        <axis>
            <xyz>0 0 1</xyz>
        </axis>
    </joint>
    <joint type="revolute" name="left_front_wheel_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>left_front_wheel</child>
        <parent>chassis</parent>
        <axis>
            <xyz>0 0 1</xyz>
        </axis>
    </joint>
    <joint type="revolute" name="left_rear_wheel_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>left_rear_wheel</child>
        <parent>chassis</parent>
        <axis>
            <xyz>0 0 1</xyz>
        </axis>
    </joint>
    <joint type="fixed" name="vertical_arm_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>vertical_arm</child>
        <parent>chassis</parent>
    </joint>
    <joint type="fixed" name="arm_extension_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>arm_extension</child>
        <parent>vertical_arm</parent>
    </joint>
    <joint type="revolute" name="junction_sphere_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>camera_junction_sphere</child>
        <parent>arm_extension</parent>
        <axis>
            <xyz>0 1 0</xyz>
        </axis>
    </joint>
    <joint type="revolute" name="camera_support_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>camera_support</child>
        <parent>camera_junction_sphere</parent>
        <axis>
            <xyz>1 0 0</xyz>
        </axis>
    </joint>
    
    <!--             Sensors                -->
    
     
    <joint name="hokuyo_joint" type="fixed">
        <child>kobra_hokuyo::link</child>
        <parent>chassis</parent>
    </joint>
    
    <joint name="camera_joint" type="fixed">
        <child>kobra_camera::link</child>
        <parent>camera_support</parent>
    </joint>
    
    <model name="kobra_hokuyo">
        <pose>0.29 0 0.10 0 0 0</pose>
        <link name="link">
        <inertial>
            <mass>0.1</mass>
        </inertial>
        <visual name="visual">
            <geometry>
            <mesh>
                <uri>model://hokuyo/meshes/hokuyo.dae</uri>
            </mesh>
            </geometry>
        </visual>
        <sensor name="laser" type="ray">
            <pose>0.01 0 0.03 0 -0 0</pose>
            <ray>
            <scan>
                <horizontal>
                <samples>640</samples>
                <resolution>1</resolution>
                <min_angle>-1.5708</min_angle>
                <max_angle>1.5708</max_angle>
                </horizontal>
            </scan>
            <range>
                <min>0.08</min>
                <max>10</max>
                <resolution>0.01</resolution>
            </range>
            </ray>

            <plugin name="laser" filename="libRayPlugin.so" />
            <always_on>1</always_on>
            <update_rate>30</update_rate>
            <visualize>true</visualize>
        </sensor>
        </link>
    </model>

    <model name="kobra_camera">
        <pose>0.229 0 1.090 0 0.52359 0</pose>
        <link name="link">
            <inertial>
            <mass>0.1</mass>
            <inertia>
                <ixx>0.000166667</ixx>
                <iyy>0.000166667</iyy>
                <izz>0.000166667</izz>
            </inertia>
            </inertial>
            <collision name="collision">
            <geometry>
                <box>
                <size>0.01 0.03 0.01</size>
                </box>
            </geometry>
            </collision>
            <visual name="visual">
            <geometry>
                <box>
                <size>0.01 0.03 0.01</size>
                </box>
            </geometry>
            </visual>
            <sensor name="camera" type="camera">
            <camera>
                <horizontal_fov>1.047</horizontal_fov>
                <image>
                <width>320</width>
                <height>240</height>
                </image>
                <clip>
                <near>0.1</near>
                <far>100</far>
                </clip>
            </camera>
            <always_on>1</always_on>
            <update_rate>30</update_rate>
            <visualize>true</visualize>
            </sensor>
        </link>
    </model>
    
    <!--            Plugins             -->
    
    <plugin name="skid_steer_drive_controller" filename="libSkidSteerDrivePlugin.so">
        <updateRate>100.0</updateRate>
        <robotNamespace>/</robotNamespace>
        <leftFrontJoint>left_front_wheel_joint</leftFrontJoint>
        <rightFrontJoint>right_front_wheel_joint</rightFrontJoint>
        <leftRearJoint>left_rear_wheel_joint</leftRearJoint>
        <rightRearJoint>right_rear_wheel_joint</rightRearJoint>
        <wheelSeparation>0.035</wheelSeparation>
        <wheelDiameter>0.145</wheelDiameter>
        <robotBaseFrame>chassis</robotBaseFrame>
        <torque>20</torque>
        <topicName>kobra_velocity</topicName>
        <broadcastTF>false</broadcastTF>
    </plugin>
    
</model>
</sdf>


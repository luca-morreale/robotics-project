<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include> 
      <uri>model://ground_plane</uri>
    </include>


  <model name="Kobra">
    
    <link name="chassis">
    <!-- Offset the base by slightly more then half the height of the wheel -->
        <pose>0 0 0.15 0 0 0</pose>
	<inertial>
        <mass>14.0</mass>
	<!-- matrix of inertias of a solid cuboid with mass m, width w, 
	     height h, depth d is:
		|m(h^2+d^2)/12		0		0    |
		|	0	m(w^2+d^2)/12		0    |
		|	0		0	m(w^2+h^2)/12| 
	-->
        <inertia>
          <ixx>1.0</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0,3236625</iyy>
          <iyz>0.0</iyz>
          <izz>1.0</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
	  <!-- Dimensions provided by Nuzoo Robotics -->
            <size>0.6 0.495 0.15</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
	  <!-- Dimensions provided by Nuzoo Robotics -->
            <size>0.6 0.495 0.15</size>
          </box>
        </geometry>
      </visual>
    </link>

    <link name="vertical_arm">
    <!-- Offset the arm by half the height of the wheel +
      + the height of the chassis -->
      <pose>0.1625 0 0.675 0 0 0</pose>
      <inertial>
        <mass>1</mass>
	<!-- matrix of inertias of a solid cylinider with mass m,
	     radius r, height h is:
		|m(3*r^2+h^2)/12	0	   0    |
		|	0	m(3*r^2+h^2)/12	   0    |
		|	0		0	m*r^2/2 | 
	-->
        <inertia>
          <ixx>0.083125</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.083125</iyy>
          <iyz>0.0</iyz>
          <izz>0.78125</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
        <cylinder>
          <radius>.025</radius>
          <length>.9</length>
	 </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
        <cylinder>
          <radius>.025</radius>
          <length>.9</length>
	 </cylinder>
        </geometry>
      </visual>
    </link> 
    
    <link name="camera_support">
      <pose>0.1875 0 1.0975 0 -1.5707 0</pose>
            <inertial>
        <mass>1</mass>
	<!-- matrix of inertias of a solid cylinider with mass m,
	     radius r, height h is:
		|m(3*r^2+h^2)/12	0	   0    |
		|	0	m(3*r^2+h^2)/12	   0    |
		|	0		0	m*r^2/2 | 
	-->
        <inertia>
          <ixx>0.083125</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.083125</iyy>
          <iyz>0.0</iyz>
          <izz>0.78125</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
        <cylinder>
          <radius>.01</radius>
          <length>.03</length>
	 </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
        <cylinder>
          <radius>.01</radius>
          <length>.03</length>
	 </cylinder>
        </geometry>
      </visual>
    </link>

        <link name="camera_support_sphere">
      <pose>0.211 0 1.0975 0 0 0</pose>
      <inertial>
      </inertial>
      <collision name="collision">
        <geometry>
        <sphere>
          <radius>.0125</radius>
	</sphere>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
        <sphere>
          <radius>.0125</radius>
	</sphere>
        </geometry>
      </visual>
    </link>
    
        <link name="camera_support_2">
      <pose>0.222 0 1.0925 0 2.0943 0</pose>
            <inertial>
        <mass>1</mass>
	<!-- matrix of inertias of a solid cylinider with mass m,
	     radius r, height h is:
		|m(3*r^2+h^2)/12	0	   0    |
		|	0	m(3*r^2+h^2)/12	   0    |
		|	0		0	m*r^2/2 | 
	-->
        <inertia>
          <ixx>0.083125</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.083125</iyy>
          <iyz>0.0</iyz>
          <izz>0.78125</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
        <cylinder>
          <radius>.01</radius>
          <length>.01</length>
	 </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
        <cylinder>
          <radius>.01</radius>
          <length>.01</length>
	 </cylinder>
        </geometry>
      </visual>
    </link>
    
    <link name="right_front_wheel">
      <!-- x y z roll pitch yaw 
      roll 90Â°=1.5707 rad      
      -->
      <pose>0.09 -0.2825 0.0725 1.5707 0 0</pose>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.0725</radius>
            <length>0.045</length>

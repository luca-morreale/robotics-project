      <child>left_rear_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>
    <joint type="fixed" name="vertical_arm_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>vertical_arm</child>
      <parent>chassis</parent>
    </joint>
        <joint type="fixed" name="camera_support_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>camera_support</child>
      <parent>vertical_arm</parent>
    </joint>
    <joint type="fixed" name="camera_support_sphere_joint">
      <pose>0 0 0 0 0 0</pose>

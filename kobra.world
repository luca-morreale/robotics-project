    </joint>
    <joint type="revolute2" name="camera_support_2_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>camera_support_2</child>
      <parent>camera_support_sphere</parent>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
      <axis2>
        <xyz>0 1 0</xyz>
      </axis2>
    </joint>

      <include>
        <uri>model://hokuyo</uri>
	<pose>0.29 0 0.10 0 0 0</pose>
      </include>
      
      <joint name="hokuyo_joint" type="fixed">
        <child>hokuyo::link</child>
        <parent>chassis</parent>
      </joint> 
      
      <include>
        <uri>model://camera</uri>
	<pose>0.522 0 1.0925 0 0.52359 0</pose>
      </include>
      
  </model>
